    "#plot survivalbility based on class category\n",
    "cs = sns.factorplot(\"Pclass\", \"Survived\", data=df_titanic, size=4)\n",
    "cs.despine(left=True)\n",
    "cs.set_ylabels(\"survival rate\")"
   ]
